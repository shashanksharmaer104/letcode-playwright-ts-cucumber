{
    "default": {
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "paths": [
            "src/test/features/addToCart.feature",
            "src/test/features/login.feature"
        ],
        "publishQuiet": true,
        "dryRun": false,
        "require": [
            "src/test/steps/*.ts",
            "src/hooks/hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "html:test-reports/cucumber-report.html",
            "json:test-reports/cucumber-report.json",
            "rerun:@rerun.txt"
        ],
        "parallel": 2
    },
    "rerun": {
        "formatOptions": {
            "snippetInterface": "async-await"
        },
        "publishQuiet": true,
        "dryRun": false,
        "require": [
            "src/test/steps/*.ts",
            "src/hooks/hooks.ts"
        ],
        "requireModule": [
            "ts-node/register"
        ],
        "format": [
            "html:test-reports/cucumber-report.html",
            "json:test-reports/cucumber-report.json",
            "rerun:@rerun.txt"
        ],
        "parallel": 2
    }
}